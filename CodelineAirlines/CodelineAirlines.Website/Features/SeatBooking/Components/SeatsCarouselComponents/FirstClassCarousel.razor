    <MudCarouselItem Transition="Transition.Fade" Color="@Color.Secondary">
        <div class="d-flex" style="height:100%">
            <MudImage Src="/Images/SeatMap/FirstClass/first2.jpg" Class="carousel-classimage" />
        </div>
    </MudCarouselItem>
    <MudCarouselItem Transition="Transition.Fade">
        <div class="d-flex" style="height:100%">
            <MudImage Src="/Images/SeatMap/FirstClass/first3.jpg" Class="carousel-classimage" />
